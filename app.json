{
  "expo": {
    "name": "MAHTO Land & Properties",
    "slug": "mlp",
    "owner": "mahtobuild",
    "version": "1.1.16",
    "extra": {
      "eas": {
        "projectId": "6f491b62-838e-45d4-bd82-0a4c932c9494"
      }
    },
    "orientation": "portrait",
    "icon": "./assets/images/launcher_icon.png",
    "scheme": "mlpapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/launcher_icon.png"
    },
    "android": {
      "versionCode": 16,
      "package": "com.mahto.landproperties",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/launcher_icon.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash_new.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#ffffff"
          }
        }
      ]
    ]
  }
}